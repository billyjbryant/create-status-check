{
  "name": "github-status-action",
  "version": "1.1.0",
  "private": true,
  "description": "Github status action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "test": "ava",
    "pack": "ncc build",
    "all": "npm run build && npm test",
    "push": "npm run build && npm run pack && git add * && git commit -m \"ci: update built file\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/billyjbryant/github-status-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "BillyJBryant <3013565+billyjbryant@users.noreply.github.com>",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@octokit/rest": "^20.0.2"
  },
  "devDependencies": {
    "@octokit/types": "^12.2.0",
    "@types/node": "^20.9.0",
    "@vercel/ncc": "^0.38.1",
    "ava": "^5.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
